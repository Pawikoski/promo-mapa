{
  "manifest_version": 3,
  "name": "OLX map",
  "version": "0.1.0",
  "description": "olx map",
  "content_scripts": [
    {
      "matches": [
        "https://www.olx.pl/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "dist/injected.js"
      ],
      "matches": [
        "https://www.olx.pl/*"
      ]
    }
  ]
}
